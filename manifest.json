{
  "name": "quotes",
  "manifest_version": 2,
  "version": "1.0",
  "description": "My quote of the day chrome extension.",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "background":{
    "page":"index.html"
  },
  
  "permissions": [
  	"notifications"
  ],

  "web_accessible_resources": [
    "background.png"
  ]
}